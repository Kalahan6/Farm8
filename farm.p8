version 42
__lua__
function _init()
	mode="game"
	itiles()
	iplr()
	itools()
end


function _update()
	utiles()
	uplr()
	utools()
end

function _draw()
	cls(3)
	map()
	dtiles()
	dplr()
	dtools()
end

function draw_obj(obj)
	spr(obj.spr,obj.x,obj.y)
end
-->8
-- player --

function iplr()
	plr={
		x=56,
		y=56,
		spd=1,
		tool="all",
		spr=1
	}
end

function uplr()
	--movement
	if (btn(⬆️)) plr.y-=plr.spd
	if (btn(➡️)) plr.x+=plr.spd
	if (btn(⬇️)) plr.y+=plr.spd
	if (btn(⬅️)) plr.x-=plr.spd
	for x=0,15 do
		for y=0,15 do
			mset(x,y,80)
		end
	end
	mset((plr.x+0)/8,(plr.y+2)/8,64)
	
	--animate
	if sin(time())>0 then plr.spr=1 else plr.spr=17 end
	
end

function dplr()
	draw_obj(plr)
end
-->8
-- tiles --

function itiles()
	tiles={}
	for mx=1,15 do
		for my=1,15 do
			tile={
				x=mx,
				y=my,
				spr_back=97,
				spr_front=66,
				crop="none"
				crop_t=0
				wtr=false
				wtr_t=0
			}
			add(tiles,tile)
		end
	end
end

function utiles()

end

function dtiles()
	for tile in all(tiles) do
		mset(tile.x,tile.y,tile.spr_back)
		mset(tile.x,tile.y,tile.spr_back)
	end
end
